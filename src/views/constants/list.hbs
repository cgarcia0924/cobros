<!-- Begin Page Content -->
<div class="container-fluid">

  <!-- Page Heading -->
  <p class="mb-4"></p> <!-- Margen Inicial  -->
  <div class="card shadow mb-4">
    <div class="card-header py-3">
      <h6 class="m-0 font-weight-bold text-primary">Adiministrar Constantes</h6>
    </div>
    <div class="pt-3 pl-3">
      <a href="/constants/add" class="btn btn-success .btn-circle btn-sm mr-auto">
        <span class="icon text-white-50">
          <i class="fas fa-check"></i>
        </span>
        <span class="text">Adicionar</span>
      </a>
    </div>
    <div class="card-body">
      <div class="table-responsive">
        <table id="example" class="table table-striped table-bordered" style="width:100%">
          <thead>
            <tr>
              <th>Id</th>
              <th>Nombre</th>
              <th>Utilidad</th>
              <th>Plazo</th>
              <th>DÃ­as Clavo</th>
              <th>Oficina</th>
              <th>Acciones</th>
            </tr>
          </thead>
          <tbody>
            {{#each constants}}
            <tr>
              <td>{{id}}</td>
              <td>{{name}}</td>
              <td>{{utility}}</td>
              <td>{{term}}</td>
              <td>{{nailday}}</td>
              <td>{{names}}</td>
              <td>
                <a class="btn btn-warning btn-sm" href="/constants/edit/{{id}}">
                  <span class="icon text-white-50"><i class="fas fa-check"></i></span>
                  <span class="text" class="d-inline-block" tabindex="0" data-placement="top" title="Editar"> </span>
                </a>
                <a class="btn btn-danger btn-sm" href="javascript:EliminarConstante({{id}})">
                  <span class="icon text-white-50 d-inline-block" tabindex="0" data-placement="top" title="Eliminar"><i
                      class="fas fa-trash" data-toggle="tooltip"> </i> </span>
                  <span class="text"> </span>
                </a>
              </td>
            </tr>
            {{/each}}

          </tbody>
          <tfoot>
            <tr>
              <th>Id</th>
              <th>Nombre Negocio</th>
              <th>Oficina</th>
              <th>Fecha Ingreso</th>
              <th>Usuario</th>
              <th>Acciones</th>
              <th>Acciones</th>
            </tr>
          </tfoot>
        </table>

      </div>
    </div>
  </div>
</div>